{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgresConnection": "Host=localhost;Port=5432;Database=IdentityDb;Username=your_user;Password=your_password"
  },
  "JWT": {
    "Issuer": "your_issuer",
    "Audience": "your_audience",
    "Key": "your_secret_key",
    "RefreshTokenValidityInDays": 7
  },
  "ConsulConfig": {
    "Address": "http://localhost:8500"
  }
}
